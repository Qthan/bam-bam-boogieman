function f(int) returns (int);
function g(int, int) returns (bool);

procedure main()
{
  var i, j, k: int;

  i, j, k := 0, 0, 0;

  goto $head.0;

$head.0:
  assert g(i,j);
  goto $body.0, $post.0;

$body.0:
  assume (i < 10);
  i, j := f(i), f(j);
  goto $head.0;

$post.0:
  assume !(i < 10);
  goto $head.1;

$head.1:
  goto $body.1, $post.1;

$body.1:
  assume (k < 1);
  k := f(k);
  goto $head.1;

$post.1:
  assume !(k < 1);
  assert false;

}
